    <header>
				<div class="header-wrap">
					{% if pageHome == "true" %}
					<h1 class="site-name"><a href="{{ '/' | url }}">{{ metadata.title }}</a></h1>
					{% else %}
					<p class="site-name"><a href="{{ '/' | url }}">{{ metadata.title }}</a></p>
					{%endif%}
      	{#- Read more about `eleventy-navigation` at https://www.11ty.dev/docs/plugins/navigation/ #}

				<a href="#" class="nav-toggle">{{ '/src/assets/icons/menu.svg' | svgContents("icon") | safe}}</a>

				<ul class="nav">
      	{%- for entry in collections.all | eleventyNavigation %}
      	  <li class="nav-item{% if entry.url == page.url %} nav-item-active{% endif %}"><a href="{{ entry.url | url }}">{{ entry.title }}</a></li>
      	{%- endfor %}
      	</ul>
				<a class="theme-switch-btn" aria-label="Theme Switch">{{ '/src/assets/icons/paintbrush.svg' | svgContents("icon") | safe}}</a>
			</div>
    </header>